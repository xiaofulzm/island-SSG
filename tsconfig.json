{
  "compilerOptions": {
    "module": "ESNext", // 模块格式
    "outDir": "dis", // 输出目录  
    "rootDir": ".", // 入口目录
    "target": "ESNext", // 指定ECMAScript目标版本
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "paths": {
      "shared/*": [
        "shared/*",
      ],
      "@runtime": [
        "runtime/index.ts"
      ]
    },
    "baseUrl": "src"
  }
}